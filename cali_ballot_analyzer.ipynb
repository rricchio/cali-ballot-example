    "import re\n",
    "import pandas as pd\n",
    "import textstat\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# List of county names (add more as needed)\n",
    "county_names = [\n",
    "    \"Alameda\", \"Butte\", \"Colusa\", \"Contra Costa\", \"El Dorado\", \"Fresno\", \"Humboldt\", \"Inyo\", \"Kern\", \"Kings\", \n",
    "    \"Lassen\", \"Los Angeles\", \"Marin\", \"Mendocino\", \"Mono\", \"Merced\", \"Monterey\", \"Orange\", \"Placer\", \"Riverside\", \"Sacramento\",\n",
    "    \"San Bernadino\", \"San Diego\", \"San Joaquin\", \"San Mateo\", \"Santa Barbara\", \"Santa Cruz\", \"Solano\", \"Sonoma\", \"Stanislaus\", \n",
    "    \"Sutter\", \"Tulare\", \"Ventura\", \"Yolo\"\n",
    "]\n",
    "\n",
    "# Function to remove county names\n",
    "def remove_counties(text):\n",
    "    county_pattern = re.compile(r'\\b(' + '|'.join(map(re.escape, county_names)) + r')\\b', re.IGNORECASE)\n",
    "    return county_pattern.sub('', text).strip()\n",
    "\n",
    "# Function to remove name-like patterns\n",
    "def remove_names(text):\n",
    "    name_pattern = re.compile(r'\\b([A-Z][a-z]+(?:\\s[A-Z][a-z]+)*|[A-Z]+)\\b')  # Captures names\n",
    "    return name_pattern.sub('', text).strip()\n",
    "\n",
    "# Function to remove all numbers\n",
    "def remove_numbers(text):\n",
    "    return re.sub(r'\\d+', '', text).strip()\n",
    "\n",
    "# Function to clean text while keeping punctuation\n",
    "def clean_text(text):\n",
    "    text = re.sub(r'[^A-Za-z\\s\\.\\,\\?\\!\\;]', ' ', text)  # Keep punctuations like . , ? !\n",
    "    text = re.sub(r'\\s+', ' ', text).strip()  # Normalize spaces\n",
    "    return text\n",
    "\n",
    "# Function to calculate readability while handling short/empty texts\n",
    "def calculate_readability(text):\n",
    "    if not text.strip() or len(text.split()) < 5:  # Less than 5 words\n",
    "        return None\n",
    "    return textstat.flesch_reading_ease(text)\n",
    "\n",
    "# Apply transformations\n",
    "cali['cleaned_body.copy'] = cali['body.copy'].apply(remove_counties)\n",
    "cali['cleaned_body.copy0'] = cali['cleaned_body.copy'].apply(remove_names)\n",
    "cali['cleaned_body.copy1'] = cali['cleaned_body.copy0'].apply(remove_numbers)\n",
    "cali['cleaned_body.copy2'] = cali['cleaned_body.copy1'].apply(clean_text)\n",
    "\n",
    "# Calculate readability scores\n",
    "cali['readability'] = cali['cleaned_body.copy2'].apply(calculate_readability)\n",
    "\n",
    "# Drop rows with missing readability scores\n",
    "cali = cali.dropna(subset=['readability'])\n",
    "\n",
    "# Print the DataFrame to inspect results\n",
    "print(cali[['body.copy', 'cleaned_body.copy2', 'readability']])\n",
    "\n",
    "# Plot the readability scores\n",
    "plt.hist(cali['readability'], bins=20, color=\"skyblue\", edgecolor=\"black\")\n",
    "plt.title(\"Distribution of Readability Scores\")\n",
    "plt.xlabel(\"Readability Score\")\n",
    "plt.ylabel(\"Frequency\")\n",
    "plt.show()\n"
   ]
