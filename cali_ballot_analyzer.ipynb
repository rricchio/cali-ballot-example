   "source": [
    "from collections import Counter\n",
    "import sklearn\n",
    "import matplotlib.pyplot as plt\n",
    "from sklearn.feature_extraction.text import CountVectorizer\n",
    "import pandas as pd\n",
    "\n",
    "# Assuming 'mnb' is your DataFrame\n",
    "# Word frequencies (ngram_range from 1 to 3, max 20 features)\n",
    "vectorizer = CountVectorizer(ngram_range=(1, 3), max_features=20)\n",
    "X = vectorizer.fit_transform(cali[\"cleaned_body.copy\"])\n",
    "\n",
    "# Sum the counts of each word/ngram\n",
    "word_counts = X.sum(axis=0).A1\n",
    "words = vectorizer.get_feature_names_out()\n",
    "\n",
    "# Sort the words by frequency (descending)\n",
    "sorted_word_counts = sorted(zip(word_counts, words), reverse=True)\n",
    "\n",
    "# Top 10 most frequent words/ngrams\n",
    "top_10_words = sorted_word_counts[:10]\n",
    "top_10_counts, top_10_words = zip(*top_10_words)\n",
    "\n",
    "# Create bar chart for top 10 words/ngrams\n",
    "plt.barh(top_10_words, top_10_counts)\n",
    "plt.title(\"Top 10 Most Frequent Words/N-Grams\")\n",
    "plt.xlabel(\"Frequency\")\n",
    "plt.show()\n"
   ]
